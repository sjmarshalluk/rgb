
<%= simple_form_for @brewery do |f| %>
	<%= f.input :name %>
	<%= f.input :address %>
	<%= f.input :brewery_type %>
	<%= f.input :description %>
	<%= f.input :year_opened %>
	<%= f.input :head_brewer %>
	<%= f.input :founder %>
	<%= f.input :core_range %>
	<%= f.input :seasonals %>
	<%= f.input :changing %>
	<%= f.input :where_to_buy %>
	<%= f.input :website %>
	<%= f.input :twitter %>
	<%= f.input :facebook %>
	<%= f.input :instagram %>
	<%= f.input :untappd %>
	<%= f.input :ratebeer %>
	<%= f.input :drink_at_brewery %>
	<%= f.input :drink_at_brewery_description %>

	<%= f.submit %>
<% end %>



